<% images.items.forEach(function(item, index) { %>
    <a href="#foto-<%=index%>" class="iniline">
        <%- image_crop(item.image, 90, 90) %>
    </a>
    <div id="foto-<%=index%>" class="hide">
        <div class="foto-container">
    		<%- image_crop(item.image, 400, 400) %>
            <div class="share">
                <% if (userId && !item.voted) { %>
                    <a data-id="<%= item.id%>" href="javascript: vote('<%= item.id%>')" class="votar">votar</a>
                <% } %>
        		<a data-id="<%= item.id%>" href="javascript: share(<%=index%>, '<%= item.id%>')" class="compartir">compartir</a>
            </div>
        </div>
	</div>
<%})%>